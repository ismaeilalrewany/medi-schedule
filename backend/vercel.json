{
  "version": 2,
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "methods": ["GET", "POST", "PUT", "DELETE"],
      "headers": {
        "access-control-allow-origin": "https://medi-schedule-epst.vercel.app"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/index.js"
    }
  ]
}